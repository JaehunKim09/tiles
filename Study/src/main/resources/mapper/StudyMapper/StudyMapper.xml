<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- SQL Mapping -->
<mapper namespace="com.spring.study.StudyDAO">
	<insert id="LoginId" parameterType="map">
		INSERT INTO Login(Lnum,UserId, pwd)
		VALUES(Lnum_SEQ.NEXTVAL,#{UserId},#{pwd})
	</insert>
</mapper>