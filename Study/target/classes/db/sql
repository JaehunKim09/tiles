CREATE TABLE JOIN
(
JNUM NUMBER PRIMARY KEY,
FIRSTNAME VARCHAR2(10),
LASTNAME VARCHAR2(10),
EMAIL VARCHAR2(40) NOT NULL,
PWD VARCHAR2(20) NOT NULL,
REPEATPWD VARCHAR2(20) NOT NULL
);

CREATE TABLE LOGIN
(
LNUM NUMBER PRIMATY KEY,
EMAIL VARCHAR2(40),
PWD VARCHAR2(20) 
);

CREATE TABLE SEARCH
(
SNUM NUMBER PRIMARY KEY,
EMAIL VARCHAR2(40) ,
SEARCH VARCHAR2(40)
);

CREATE TABLE EVENT
(
ENUM NUMBER PRIMARY KEY,
CLICK VARCHAR2(20)
);

